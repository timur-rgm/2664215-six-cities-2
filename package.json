{
  "name": "six-cities",
  "version": "5.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "start:dev": "nodemon",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "lint:openapi": "openapi validate specification/specification.yml",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "mock:server": "json-server ./mocks/mock-server-data.json --port 3123",
    "generate:mocks": "ts-node ./src/main.cli.ts --generate 100 ./mocks/offers.tsv http://localhost:3123/api",
    "import:mocks": "ts-node ./src/main.cli.ts --import ./mocks/offers.tsv admin test 127.0.0.1 27017 six-cities \"1aZk6pD&Zx\""
  },
  "devDependencies": {
    "@redocly/openapi-cli": "0.12.16",
    "@types/convict": "6.1.6",
    "@types/convict-format-with-validator": "6.0.5",
    "@types/express": "4.17.21",
    "@types/mime-types": "2.1.2",
    "@types/multer": "1.4.8",
    "@types/node": "18.17.17",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "0.17.3",
    "nodemon": "3.0.1",
    "pino-pretty": "10.3.1",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.8.1",
    "typescript": "5.2.2"
  },
  "type": "module",
  "engines": {
    "node": "18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "@typegoose/typegoose": "11.8.1",
    "chalk": "5.4.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "dayjs": "1.11.13",
    "dotenv": "16.6.1",
    "express": "4.21.2",
    "express-async-handler": "1.2.0",
    "got": "13.0.0",
    "http-status-codes": "2.3.0",
    "inversify": "6.2.2",
    "jose": "4.15.4",
    "mime-types": "2.1.35",
    "mongoose": "7.8.7",
    "multer": "1.4.5-lts.1",
    "nanoid": "5.1.6",
    "pino": "8.21.0",
    "reflect-metadata": "0.2.2"
  }
}
